{
  "name": "swlite",
  "version": "0.0.1",
  "description": " ",
  "main": "index.js",
  "sideEffects": false,
  "scripts": {
    "start": "rspack serve -c config/compiler/rspack.dev.js",
    "build": "npm run clean && rspack --mode=production -c config/compiler/rspack.prod.js",
    "build:pipe": "rspack --mode=production -c config/compiler/rspack.prod.js",
    "clean": "rimraf build/*",
    "lint": "eslint src  --ext .ts --ext .tsx",
    "prettier:check": "prettier --check src/",
    "stylelint": "stylelint src/**/*.module.css",
    "lint:fix": "eslint --fix --ext .ts --ext .tsx",
    "format": "prettier --write \"src/**/*.{ts,tsx}\"",
    "lhci": "lhci autorun",
    "ts:check": "tsc",
    "ts:watch": "tsc -w  --incremental",
    "test": "jest --silent --colors --coverage --maxWorkers=60%",
    "test:watch": "jest --watchAll",
    "build:analyze": "rspack --mode=production -c config/compiler/rspack.analyze.js",
    "build:analyze:cicd": "APP_ENV=CICD rspack --mode=production -c config/compiler/rspack.analyze.js",
    "quality:check": "npm run lint && npm run stylelint && npm run prettier:check && npm run ts:check",
    "prepare": "husky install",
    "test:diff": "jest --coverage --changedSince=develop",
    "build-components": "rimraf build/* && rollup -c",
    "rsdoctor:build": "RSDOCTOR=true pnpm run build",
    "update-pkgs": "npx lsq update-all-pkgs @lsq/nextgen-preact -yop"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@lsq/nextgen-preact": "^0.249.0",
    "@preact/compat": "^17.1.2",
    "dompurify": "^3.0.6",
    "file-saver": "2.0.5",
    "froala-editor": "^4.1.3",
    "idb-keyval": "^6.2.1",
    "immer": "10.1.1",
    "jszip": "3.10.1",
    "preact": "^10.18.1",
    "react-beautiful-dnd": "^13.1.1",
    "react-froala-wysiwyg": "^4.1.3",
    "react-resizable": "^3.0.5",
    "stream": "^0.0.2",
    "tributejs": "^5.1.3",
    "wouter": "^2.12.1",
    "zod": "^3.24.2",
    "zustand": "^4.5.5"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.8.0",
    "@commitlint/config-conventional": "^17.8.0",
    "@lhci/cli": "^0.12.0",
    "@rollup/plugin-alias": "^5.1.0",
    "@rollup/plugin-commonjs": "^26.0.1",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rsdoctor/cli": "^0.3.9",
    "@rsdoctor/rspack-plugin": "^0.3.9",
    "@rspack/cli": "0.7.5",
    "@rspack/core": "0.7.5",
    "@testing-library/jest-dom": "^6.1.4",
    "@testing-library/react": "^14.0.0",
    "@testing-library/react-hooks": "^8.0.1",
    "@types/jest": "^29.5.6",
    "@types/node": "^20.8.7",
    "@types/react": "^18.2.29",
    "@types/react-beautiful-dnd": "^13.1.7",
    "@types/react-dom": "^18.2.14",
    "@typescript-eslint/eslint-plugin": "^6.8.0",
    "@typescript-eslint/parser": "^6.8.0",
    "autoprefixer": "^10.4.16",
    "conventional-changelog-conventionalcommits": "^7.0.2",
    "eslint": "8.51.0",
    "eslint-config-airbnb-typescript": "^17.1.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-jest": "^27.4.2",
    "eslint-plugin-module-resolver": "^1.4.0",
    "eslint-plugin-preact": "^0.1.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "husky": "^8.0.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jest-stare": "^2.5.1",
    "jest-transform-stub": "^2.0.0",
    "jest-watch-typeahead": "^2.2.2",
    "json-loader": "^0.5.7",
    "lint-staged": "^15.0.2",
    "postcss": "^8.4.31",
    "postcss-loader": "^7.3.3",
    "postcss-nested": "^6.0.1",
    "prettier": "^3.0.3",
    "react-app-polyfill": "^3.0.0",
    "rimraf": "^5.0.5",
    "rollup": "^4.21.0",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-postcss": "^4.0.2",
    "rollup-plugin-styles": "^4.0.0",
    "rollup-plugin-svg": "^2.0.0",
    "rollup-plugin-typescript2": "^0.36.0",
    "stylelint": "^15.11.0",
    "stylelint-config-standard": "^34.0.0",
    "ts-jest": "^29.1.1",
    "typescript": "^5.8.2",
    "typescript-plugin-css-modules": "^5.0.2",
    "url-loader": "^4.1.1",
    "webpack-bundle-analyzer": "^4.9.1",
    "webpack-merge": "^5.10.0"
  },
  "lint-staged": {
    "**/*.{js,jsx,ts,tsx}": [
      "npm run lint"
    ],
    "src/**/*.module.css": [
      "npm run stylelint"
    ]
  }
}
